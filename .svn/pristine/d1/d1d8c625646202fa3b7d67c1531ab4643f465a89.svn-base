package com.fc.member.store.user.entity;

import java.io.Serializable;
import java.math.BigDecimal;

/**
 * <p>Table: StoreBorrowDetail</p>
 * <p>Description: </p>
 * @author yangd
 * @date 2018-12-27 16:31:05
 */
public class StoreBorrowDetail implements Serializable {

	private static final long serialVersionUID = 1L;

	private Integer storeBorrowDetailId; // 门店借款记录id
	private Integer storeId; // 门店id
	private String borrowNo; // 借款单号
	private String orderNo; // 订单号
	private BigDecimal borrowMoney; // 预借金额
	private BigDecimal waitRepayMoney; // 待还金额
	private String purpose; // 用途
	private Integer borrowTime; // 预借时间
	private String remark; // 备注

	public void setStoreBorrowDetailId(Integer storeBorrowDetailId) {
		this.storeBorrowDetailId = storeBorrowDetailId;
	}

	public Integer getStoreBorrowDetailId() {
		return this.storeBorrowDetailId;
	}

	public void setStoreId(Integer storeId) {
		this.storeId = storeId;
	}

	public Integer getStoreId() {
		return this.storeId;
	}

	public void setBorrowNo(String borrowNo) {
		this.borrowNo = borrowNo;
	}

	public String getBorrowNo() {
		return this.borrowNo;
	}

	public void setOrderNo(String orderNo) {
		this.orderNo = orderNo;
	}

	public String getOrderNo() {
		return this.orderNo;
	}

	public void setBorrowMoney(BigDecimal borrowMoney) {
		this.borrowMoney = borrowMoney;
	}

	public BigDecimal getBorrowMoney() {
		return this.borrowMoney;
	}

	public void setWaitRepayMoney(BigDecimal waitRepayMoney) {
		this.waitRepayMoney = waitRepayMoney;
	}

	public BigDecimal getWaitRepayMoney() {
		return this.waitRepayMoney;
	}

	public void setPurpose(String purpose) {
		this.purpose = purpose;
	}

	public String getPurpose() {
		return this.purpose;
	}

	public void setBorrowTime(Integer borrowTime) {
		this.borrowTime = borrowTime;
	}

	public Integer getBorrowTime() {
		return this.borrowTime;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getRemark() {
		return this.remark;
	}

	@Override
	public String toString() {
		return "StoreBorrowDetail{" + "storeBorrowDetailId=" + storeBorrowDetailId + ", storeId=" + storeId
				+ ", borrowNo=" + borrowNo + ", orderNo=" + orderNo + ", borrowMoney=" + borrowMoney
				+ ", waitRepayMoney=" + waitRepayMoney + ", purpose=" + purpose + ", borrowTime=" + borrowTime
				+ ", remark=" + remark + "}";
	}
}