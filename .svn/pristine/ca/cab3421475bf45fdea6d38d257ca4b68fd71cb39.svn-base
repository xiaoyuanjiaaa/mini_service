		return this.remark;
	}
	public static StoreBankCardChange passValueToStoreBankCardChange(CreateStoreBankCardRequest request){
		StoreBankCardChange storeBankCardChange = new StoreBankCardChange();
		storeBankCardChange.setStoreId(request.getStoreId());
		storeBankCardChange.setBankHoldName(request.getBankHoldName());
		storeBankCardChange.setBankCardNo(request.getBankCardNo());
		storeBankCardChange.setCardBankName(request.getCardBankName());
		storeBankCardChange.setDepositBank(request.getDepositBank());
		storeBankCardChange.setCardAddress(request.getCardAddress());
		storeBankCardChange.setAddress("地址");
		storeBankCardChange.setRegionId(request.getRegionId());
		storeBankCardChange.setBankCode("123");
		storeBankCardChange.setRemark("修改银行卡");
		storeBankCardChange.setStatus(0);
		storeBankCardChange.setCheckTime(1212);
		storeBankCardChange.setCreateTime(1212);
//		storeBankCardChange.setUpdateTime(9999);

		return storeBankCardChange;

	}
