	    	", bankHoldName=" + bankHoldName + 
	    	", depositBank=" + depositBank + 
	    	", regionId=" + regionId + 
	    	", cardAddress=" + cardAddress + 
	    	", address=" + address + 
	    	", status=" + status + 
	    	", checkTime=" + checkTime + 
	    	", createTime=" + createTime + 
	    	", remark=" + remark + 
	    	"}";
    }
