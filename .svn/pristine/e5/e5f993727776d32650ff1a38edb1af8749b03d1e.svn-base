package com.fc.protocol.demo.request.wechatmaintain.user;

import org.springframework.util.StringUtils;

import com.fc.protocol.common.message.body.ProtocolRequest;

import io.swagger.annotations.ApiModelProperty;

/**
 * 微养车用户 根据openid查询微养车用户参数
 * @author yangd
 * @date 2019年1月8日
 */
public class WechatMaintainUserByOpenIdRequest extends ProtocolRequest {

	private static final long serialVersionUID = 1L;

	@ApiModelProperty(value = "openid", name = "openid")
	private String openid; // 微信openid

	/**
	 * 验证方法
	 * @author yangd
	 * @see update_name:yangd update_time:2019-01-08
	 * @return
	 */
	public String paramValidate() {
		if (StringUtils.isEmpty(openid)) {
			return "参数openid不能为空";
		}
		return null;
	}

	public String getOpenid() {
		return openid;
	}

	public void setOpenid(String openid) {
		this.openid = openid;
	}

	@Override
	public String toString() {
		return "wechatMaintainUserByOpenIdRequest [openid=" + openid + ", getVersion()=" + getVersion()
				+ ", getReqCode()=" + getReqCode() + ", getTraceId()=" + getTraceId() + "]";
	}

}
