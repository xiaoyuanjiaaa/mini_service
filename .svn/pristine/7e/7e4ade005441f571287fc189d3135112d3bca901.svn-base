package com.fc.protocol.demo.request;

import org.apache.commons.lang.StringUtils;

import com.fc.protocol.common.message.body.ProtocolRequest;

public class EchoRequest extends ProtocolRequest {

	/**
	 * 
	 */
	private static final long serialVersionUID = -3194798778956586300L;

	private int number;
	private String name;

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getNumber() {
		return number;
	}

	public void setNumber(int number) {
		this.number = number;
	}

	public String paramValidate() {
		if (StringUtils.isBlank(name)) {
			return "参数name不能为空";
		}
		return null;
	}

	@Override
	public String toString() {
		return "EchoRequest [number=" + number + ", getVersion()=" + getVersion() + ", getReqCode()=" + getReqCode()
				+ ", getTraceId()=" + getTraceId() + "]";
	}

}
